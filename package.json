{
  "name": "civic-impact-tickets",
  "version": "1.0.0",
  "main": "index.js",
  "description": "Decentralized Ticketing with Anti-Scalping, Universal Loyalty Rewards, and Social Impact Vendor Plugins",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "npx expo export -p web --output-dir dist",
    "build:web": "npx expo export -p web --output-dir dist",
    "serve": "npx serve dist -s -p $PORT",
    "start:prod": "serve dist -s -p $PORT",
    "render-build": "npm run build && echo 'Build completed successfully'",
    "postinstall": "npx expo install --fix"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@solana/web3.js": "^1.87.6",
    "axios": "^1.6.0",
    "expo": "~50.0.0",
    "react": "18.2.0",
    "react-native": "0.73.0",
    "react-native-gesture-handler": "^2.14.0",
    "react-native-reanimated": "^3.6.2",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "^3.29.0",
    "react-native-svg": "^14.1.0",
    "react-native-vector-icons": "^10.0.3",
    "react-native-web": "~0.19.6",
    "react-dom": "18.2.0",
    "@expo/metro-runtime": "~3.1.3",
    "serve": "^14.2.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "^18.2.45",
    "@types/react-native": "^0.73.0",
    "metro": "^0.80.0",
    "metro-resolver": "^0.80.0",
    "typescript": "^5.1.3"
  },
  "resolutions": {
    "react-native-svg": "14.1.0"
  },
  "private": true
}
